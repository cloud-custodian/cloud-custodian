[
  {
    "Id": "Cross-Account-Trust-Policy-Only-Service-Principal",
    "Version": "2012-10-17",
    "Statement": [
      {
        "Action": [
          "sts:AssumeRole"
        ],
        "Effect": "Allow",
	"Principal": {
	  "Service": "lambda.amazonaws.com"
	}
      }
    ]
  },
  {
    "Id": "Cross-Account-Trust-Policy-Service-Principal-And-More",
    "Version": "2012-10-17",
    "Statement": [
      {
        "Action": [
          "sts:AssumeRole"
        ],
        "Effect": "Allow",
	"Principal": {
	  "Service": "lambda.amazonaws.com",
	  "AWS": [ "123456789012", "098765432109" ]
	}
      }
    ]
  }
]
