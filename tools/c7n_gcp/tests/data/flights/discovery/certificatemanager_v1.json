{
  "kind": "discovery#restDescription",
  "etag": "\"YWOzh2SDasdU84ArJnpYek-OMdg/dummy-etag\"",
  "discoveryVersion": "v1",
  "id": "certificatemanager:v1",
  "name": "certificatemanager",
  "version": "v1",
  "title": "Certificate Manager API",
  "description": "The Certificate Manager API is used to acquire and manage TLS (SSL) certificates for Google Cloud Load Balancing.",
  "ownerDomain": "google.com",
  "ownerName": "Google",
  "documentationLink": "https://cloud.google.com/certificate-manager",
  "protocol": "rest",
  "baseUrl": "https://certificatemanager.googleapis.com/",
  "basePath": "",
  "rootUrl": "https://certificatemanager.googleapis.com/",
  "servicePath": "",
  "batchPath": "batch",
  "parameters": {
    "access_token": {
      "type": "string",
      "description": "OAuth access token.",
      "location": "query"
    },
    "alt": {
      "type": "string",
      "description": "Data format for response.",
      "default": "json",
      "enum": ["json", "media", "proto"],
      "enumDescriptions": ["Responses with Content-Type of application/json", "Media download with context-dependent Content-Type", "Responses with Content-Type of application/x-protobuf"],
      "location": "query"
    }
  },
  "schemas": {
    "Certificate": {
      "id": "Certificate",
      "type": "object",
      "description": "Defines TLS certificate.",
      "properties": {
        "name": {
          "type": "string",
          "description": "A user-defined name of the certificate."
        },
        "description": {
          "type": "string",
          "description": "One or more paragraphs of text description of a certificate."
        },
        "createTime": {
          "type": "string",
          "description": "The creation timestamp of a Certificate.",
          "format": "google-datetime"
        },
        "updateTime": {
          "type": "string", 
          "description": "The last update timestamp of a Certificate.",
          "format": "google-datetime"
        },
        "labels": {
          "type": "object",
          "description": "Set of labels associated with a Certificate.",
          "additionalProperties": {
            "type": "string"
          }
        },
        "certificate": {
          "$ref": "CertificateDetails"
        },
        "sanDnsnames": {
          "type": "array",
          "description": "The list of Subject Alternative Names of dnsName type defined in the certificate.",
          "items": {
            "type": "string"
          }
        },
        "scope": {
          "type": "string",
          "description": "The scope of the certificate.",
          "enum": ["DEFAULT", "EDGE_CACHE"],
          "enumDescriptions": ["The certificate is used for the purposes described in the certificate.", "The certificate is used for the edge cache."]
        }
      }
    },
    "CertificateDetails": {
      "id": "CertificateDetails",
      "type": "object",
      "description": "Certificate details.",
      "properties": {
        "pemCertificate": {
          "type": "string",
          "description": "The PEM-encoded certificate chain."
        }
      }
    },
    "ListCertificatesResponse": {
      "id": "ListCertificatesResponse",
      "type": "object",
      "description": "Response for the `ListCertificates` method.",
      "properties": {
        "certificates": {
          "type": "array",
          "description": "A list of certificates for the parent resource.",
          "items": {
            "$ref": "Certificate"
          }
        },
        "nextPageToken": {
          "type": "string",
          "description": "If there are more results than have been returned, then this field is set to a non-empty value."
        }
      }
    }
  },
  "resources": {
    "projects": {
      "resources": {
        "locations": {
          "resources": {
            "certificates": {
              "methods": {
                "list": {
                  "id": "certificatemanager.projects.locations.certificates.list",
                  "path": "v1/{+parent}/certificates",
                  "flatPath": "v1/projects/{projectsId}/locations/{locationsId}/certificates",
                  "httpMethod": "GET",
                  "description": "Lists Certificates in a given project and location.",
                  "parameters": {
                    "parent": {
                      "type": "string",
                      "description": "Required. The project and location from which the certificate should be listed, specified in the format `projects/*/locations/*`.",
                      "required": true,
                      "pattern": "^projects/[^/]+/locations/[^/]+$",
                      "location": "path"
                    },
                    "pageSize": {
                      "type": "integer",
                      "description": "Maximum number of certificates to return per call.",
                      "format": "int32",
                      "location": "query"
                    },
                    "pageToken": {
                      "type": "string",
                      "description": "The value returned by the last `ListCertificatesResponse`. Indicates that this is a continuation of a prior `ListCertificates` call.",
                      "location": "query"
                    }
                  },
                  "parameterOrder": ["parent"],
                  "response": {
                    "$ref": "ListCertificatesResponse"
                  },
                  "scopes": ["https://www.googleapis.com/auth/cloud-platform"]
                }
              }
            }
          }
        }
      }
    }
  }
}
